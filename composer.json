{
    "name": "dotkernel/dot-rbac-guard",
    "type": "library",
    "description": "DotKernel RBAC guards component",
    "license": "MIT",
    "keywords": [
        "laminas",
        "mezzio"
    ],
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "mezzio/mezzio-router": "^3.1",
        "mezzio/mezzio-template": "^2.0",
        "laminas/laminas-servicemanager": "^3.4",
        "dotkernel/dot-rbac": "^3.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "squizlabs/php_codesniffer": "^3.5",
        "laminas/laminas-diactoros": "^2.3",
        "laminas/laminas-stdlib": "^3.2",
        "dotkernel/dot-controller": "^3.0",
        "dotkernel/dot-flashmessenger": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Rbac\\Guard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Rbac\\Guard\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        }
    }
}
