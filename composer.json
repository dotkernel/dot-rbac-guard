{
    "name": "dotkernel/dot-rbac-guard",
    "type": "library",
    "description": "DotKernel RBAC guards component",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "mezzio/mezzio-router": "^3.0",
        "mezzio/mezzio-template": "^2.0",
        "laminas/laminas-servicemanager": "^3.3.0",
        "dotkernel/dot-rbac": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3",
        "laminas/laminas-diactoros": "^1.3",
        "laminas/laminas-stdlib": "^3.0",
        "dotkernel/dot-controller": "^2.0",
        "dotkernel/dot-authentication": "^2.0",
        "dotkernel/dot-flashmessenger": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Rbac\\Guard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Rbac\\Guard\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
